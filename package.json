{
  "name": "ng-recaptchav3",
  "version": "8.0.0",
  "description": "Angular component for Google reCAPTCHA v3, v2 and invisible recaptcha",
  "scripts": {
    "file-gen": "ts-node ./apps/demo/bin/file-gen.ts",
    "demo": "yarn demo:serve",
    "demo:serve": "cd demo && yarn start",
    "demo:build": "cd demo && yarn build",
    "demo:publish": "run-s demo:build demo:publish-gh-pages",
    "demo:publish-gh-pages": "gh-pages -d demo/dist/ng-recaptcha",
    "changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file",
    "github-release": "conventional-github-releaser --preset angular",
    "version": "yarn changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && run-s n pub",
    "lint": "ng lint",
    "build": "ng build",
    "pub": "run-s build publish",
    "publish": "npm publish",
    "prepush": "yarn lint"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikhere/ng-recaptcha.git"
  },
  "keywords": [
    "angular",
    "recaptcha",
    "angular-recaptcha",
    "ng-recaptchav3",
    "recaptchav3"
  ],
  "author": "Ruslan Arkhipau <dethariel@gmail.com>, Vikash Singh<singhvkash@gmail.com>",
  "license": "MIT",
